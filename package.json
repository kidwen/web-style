{
    "name": "style-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "nx --version",
        "start": "nx serve --hmr",
        "build": "nx exec -- yarn _build ",
        "build:layout": "nx exec -- nx build layout",
        "lint": "nx exec -- nx lint",
        "_build": "npm run build:layout && nx build",
        "deploy": "npm run ng deploy --base-href=/web-style/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.8",
        "@angular/cdk": "^17.0.2",
        "@angular/common": "^17.0.8",
        "@angular/compiler": "^17.0.8",
        "@angular/core": "^17.0.8",
        "@angular/forms": "^17.0.8",
        "@angular/material": "^17.0.2",
        "@angular/platform-browser": "^17.0.8",
        "@angular/platform-browser-dynamic": "^17.0.8",
        "@angular/router": "^17.0.9",
        "animate.css": "^4.1.1",
        "eva-icons": "^1.1.3",
        "rxjs": "^7.5.7",
        "tslib": "^2.6.2",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.10",
        "@angular-devkit/core": "^17.0.8",
        "@angular-devkit/schematics": "^17.0.1",
        "@angular-eslint/builder": "17.1.0",
        "@angular-eslint/eslint-plugin": "17.2.0",
        "@angular-eslint/eslint-plugin-template": "17.2.0",
        "@angular-eslint/schematics": "17.1.0",
        "@angular-eslint/template-parser": "17.2.0",
        "@angular/cli": "^17.0.9",
        "@angular/compiler-cli": "^17.1.0",
        "@angular/language-service": "^17.0.9",
        "@kidwen/eslint-config-angular": "^17.1.0",
        "@nx/angular": "17.2.8",
        "@nx/workspace": "17.2.8",
        "@schematics/angular": "^17.0.1",
        "@types/estree": "^1.0.0",
        "@types/jasmine": "^5.1.4",
        "@types/jasminewd2": "^2.0.8",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "angular-cli-ghpages": "^0.6.2",
        "codelyzer": "^6.0.1",
        "eslint": "^8.56.0",
        "jasmine-core": "^5.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "ng-packagr": "^17.0.3",
        "nx": "17.2.8",
        "protractor": "^7.0.0",
        "ts-node": "^10.9.1",
        "typescript": "5.2.2"
    },
    "engines": {
        "node": ">=16.10.0",
        "npm": "Please use yarn instead of NPM to install dependencies"
    },
    "license": "MIT",
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/dist"
                ]
            },
            "build:layout": {
                "outputs": [
                    "{projectRoot}/dist"
                ]
            }
        }
    }
}
