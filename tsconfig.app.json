{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "./out-tsc/app",
        "types": []
    },
    "files": [
        "src/main.ts",
        "src/polyfills.ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "paths": {
        "@walle/shared": [
            "src/app/shared"
        ]
    },
    "exclude": [
        "src/test.ts",
        "src/**/*.spec.ts"
    ]
}
